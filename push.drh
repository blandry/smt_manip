[-10, 10] x;
[-100, 100] xdot;
[-100, 100] xdotdot;

[0, 1] t;
[0, 1] h;

[0, 10] time;

// stationary
{ mode 1;
  invt:
        (t <= 1);
  flow:
        d/dt[x] = xdot;
        d/dt[xdot] = xdotdot;
        d/dt[xdotdot] = 0;
        d/dt[t] = h;
        d/dt[h] = 0;
  jump:
        (t >= 1) ==> @1 (and (x' = x) (xdot' = xdot) (xdotdot' < 100) (t' = 0) (h' < 1));
}

init:
@1	(and (x = 0) (xdot = 0) (xdotdot = 0) (t = 0));

goal:
@1	(and (x > 1) (xdot < 0.0001) (xdot > 0));
